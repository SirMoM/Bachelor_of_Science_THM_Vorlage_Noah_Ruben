{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/hund.json",
    "type": "object",
    "title": "Hund",
    "required": [
        "anzahl_beine",
        "fellfarbe",
        "saeugetier"
    ],
    "additionalProperties": false,
    "properties": {
        "anzahl_beine": {
            "$id": "#/properties/anzahl_beine",
            "type": "integer"
        },
        "fellfarbe": {
            "$id": "#/properties/fellfarbe",
            "type": "string"
        },
        "saeugetier": {
            "$id": "#/properties/saeugetier",
            "type": "string",
            "default": true
        }
    }
}